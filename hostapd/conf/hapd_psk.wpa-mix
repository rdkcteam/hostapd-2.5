#
driver=nl80211
interface=wlan1
ctrl_interface=/var/run/hostapd
bridge=br0
ssid=hostapd_2_5-wpa-mix

hw_mode=g
channel=1

ieee80211n=1
#ht_capab=[HT40+][SHORT-GI-20][SHORT-GI-40]
ieee80211d=1
country_code=US
ignore_broadcast_ssid=0
wmm_enabled=1



logger_syslog=127
logger_syslog_level=2
logger_stdout=127
logger_stdout_level=2


auth_algs=1
wpa=2
wpa_passphrase=12345678
wpa_key_mgmt=WPA-PSK
#wpa_pairwise=CCMP 
rsn_pairwise=CCMP


